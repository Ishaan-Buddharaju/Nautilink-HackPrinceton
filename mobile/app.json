{
  "expo": {
    "name": "Nautilink",
    "slug": "nautilink-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "nautilink",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nautilink.app",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to capture photos of your fishing catch for documentation and compliance tracking.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select photos of your catch."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#171717"
      },
      "package": "com.nautilink.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "apiUrl": "http://localhost:8000",
      "auth0Domain": "your-tenant.auth0.com",
      "auth0ClientId": "your-client-id"
    }
  }
}
